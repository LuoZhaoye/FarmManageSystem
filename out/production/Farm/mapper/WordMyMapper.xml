<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.it.mapper.WordMyMapper">
	<delete id="del">
		DELETE from gm_word_my
		WHERE
			userid = #{userid}
			and wordid = #{wordid}
	</delete>
</mapper>